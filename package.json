{
  "name": "@gnosis.pm/dx-services",
  "version": "2.3.6",
  "description": "",
  "main": "src/runPublicApi.js",
  "directories": {
    "test": "test"
  },
  "bin": {
    "dutchx-kovan": "./src/cli/cli-kovan.js",
    "dutchx-rinkeby": "./src/cli/cli-rinkeby.js",
    "dutchx-mainnet": "./src/cli/cli-mainnet.js"
  },
  "scripts": {
    "executornode:ropsten": "export ROPSTEN=ROPSTEN; npx truffle exec executornode.js --network ropsten",
    "console:dev": "npx truffle console --network development",
    "console:rinkeby": "export RINKEBY=RINKEBY; npx truffle console --network rinkeby",
    "console:mainnet": "npx truffle console --network mainnet",
    "migrate:dev": "npx truffle migrate --reset --network development",
    "prep": "mv migrations/*DEV_migrate* .",
    "post": "mv *DEV_migrate* migrations",
    "migrate:rinkeby": "yarn prep; export RINKEBY=RINKEBY; npx truffle migrate --reset --network rinkeby; yarn post",
    "migrate:mainnet": "yarn prep; npx truffle migrate --reset --network mainnet; yarn post",
    "mv-migrations": "mv migrations/* .",
    "mv-migrations-back": "mv *_migrate_* migrations/",
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:gnosis/dx-services.git"
  },
  "license": "MIT",
  "dependencies": {
    "@openzeppelin/contracts": "^2.3.0",
    "@slack/client": "^4.11.0",
    "cors": "^2.8.5",
    "coveralls": "^3.0.4",
    "debug": "^4.1.1",
    "dotenv": "^8.0.0",
    "express": "^4.16.4",
    "git-rev-sync": "^1.12.0",
    "got": "^9.6.0",
    "jest": "^24.8.0",
    "lodash.clonedeep": "^4.5.0",
    "node-cache": "^4.2.0",
    "node-schedule": "^1.3.2",
    "qs": "^6.6.0",
    "raven": "^2.6.4",
    "solc": "0.5.10",
    "truffle": "^5.0.22",
    "truffle-contract": "^3.0.6",
    "truffle-hdwallet-provider": "^1.0.5",
    "web3-provider-engine": "^14.1.0",
    "yargs": "^13.2.4"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "commander": "^2.20.0",
    "esdoc": "^1.1.0",
    "esdoc-standard-plugin": "^1.0.0",
    "eslint": "^5.16.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-jest": "^22.6.4",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-standard": "^4.0.0",
    "ganache-cli": "^6.4.3",
    "nodemon": "^1.19.1",
    "prettier-standard": "^9.1.1",
    "truffle-assertions": "^0.9.1",
    "yamljs": "^0.3.0"
  }
}
